{
    "masmtasm.ASM.emulator": "dosbox",
    "masmtasm.ASM.mode": "single file",
    "masmtasm.ASM.actions": {
        "TASM": {
            "baseBundle": "<built-in>/TASM.jsdos",
            "before": [
                "set PATH=C:\\TASM",
            ],
            "run": [
                "TASM /m2 ${file}",
                "TLINK ${filename}",
                ">${filename}"
            ],
            "debug": [
                "TASM /m2 /zi ${file} ${filename}.obj",
                "TLINK /v/3 ${filename}.obj",
                "TD ${filename}.exe"
            ]
        }
    },
    "masmtasm.ASM.assembler": "TASM"
}
